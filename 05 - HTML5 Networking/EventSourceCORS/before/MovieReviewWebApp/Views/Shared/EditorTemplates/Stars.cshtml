@model int?

<ol class="starsEditor">
@for (var i = 1; i <= 5; i++)
{
    <li>
        @{
            var id = ViewContext.ViewData.TemplateInfo.GetFullHtmlFieldId("") + "_" + i.ToString();
        }
        @Html.RadioButton("", i.ToString(), Model==i, new { id })
        <label for="@id">
            @Html.Partial("~/Views/Shared/DisplayTemplates/Stars.cshtml", i)
        </label>
    </li>
}
</ol>