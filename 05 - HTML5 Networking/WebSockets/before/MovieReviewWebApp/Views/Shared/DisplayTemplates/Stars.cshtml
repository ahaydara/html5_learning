@model int?
           
@if (Model == null)
{
    <span data-stars="0" class="stars">No Ratings</span>
}
else
{
    var val = Model.Value;
    <span title="@Model stars" data-stars="@val" class="stars">
        @for (int i = 1; i <= val; i++)
        {
            @Html.Image("~/Content/Images/Star.png")
        }
        @for (int i = val + 1; i <= 5; i++)
        {
            @Html.Image("~/Content/Images/Star-Empty.png")
        }
    </span>
}